{
  "name": "portfolio-frontend",
  "projectType": "application",
  "generators": {},
  "sourceRoot": "apps/portfolio-frontend/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/portfolio-frontend",
        "index": "apps/portfolio-frontend/src/index.html",
        "main": "apps/portfolio-frontend/src/main.ts",
        "polyfills": "apps/portfolio-frontend/src/polyfills.ts",
        "tsConfig": "apps/portfolio-frontend/tsconfig.app.json",
        "assets": [
          "apps/portfolio-frontend/src/favicon.ico",
          "apps/portfolio-frontend/src/assets"
        ],
        "styles": [
          "apps/portfolio-frontend/src/styles.css",
          "node_modules/bootstrap/dist/css/bootstrap.css"
        ],
        "scripts": [
          "node_modules/bootstrap/dist/js/bootstrap.js"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/portfolio-frontend/src/environments/environment.ts",
              "with": "apps/portfolio-frontend/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "portfolio-frontend:build:production"
        },
        "development": {
          "browserTarget": "portfolio-frontend:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "portfolio-frontend:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "apps/portfolio-frontend/src/test.ts",
        "polyfills": "apps/portfolio-frontend/src/polyfills.ts",
        "tsConfig": "apps/portfolio-frontend/tsconfig.spec.json",
        "karmaConfig": "apps/portfolio-frontend/karma.conf.js",
        "assets": [
          "apps/portfolio-frontend/src/favicon.ico",
          "apps/portfolio-frontend/src/assets"
        ],
        "styles": [
          "apps/portfolio-frontend/src/styles.css"
        ],
        "scripts": []
      }
    }
  }
}
